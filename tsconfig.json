{
    "compilerOptions": {
      "target": "es5",
      "lib": ["dom", "es6"],
      "jsx": "react",
      "moduleResolution": "node",
      "module": "esnext",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules", "build", "dist"]
}
  
  